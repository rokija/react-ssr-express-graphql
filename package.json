{
  "name": "react-ssr-graphql",
  "description": "Boilerplate for React Server Side rendered app with graphql and nexus setup and hot reloading for client",
  "version": "1.0.0",
  "engines": {
    "node": "11.14.0",
    "npm": "6.9.0"
  },
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development rm -rf dist && webpack --mode development && node ./dist/server.js",
    "start": "NODE_ENV=production rm -rf dist && webpack --mode production && node ./dist/server.js",
    "lint": "eslint src --ext .js,.jsx"
  },
  "author": "",
  "license": "ISC",
