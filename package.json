    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "babel-loader": "^8.0.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
